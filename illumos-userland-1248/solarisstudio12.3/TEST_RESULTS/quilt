while read line; do						\
	  case "$line" in						\
	  '@REFERENCE''@')						\
	    /usr/gnu/bin/make -s reference |					\
	    /usr/gnu/bin/sed -e 's/^quilt \([^ ]*\)\(.*\)/.IP "\\fB\1\\fP\2 " 4/'	\
		   -e $'s/^  \\(-[^\t]*\\)\t\\?/.IP "    \\1" 8\\\n/'	\
		   -e $'s/^  \t\\?//'					\
	    ;;								\
	  *)								\
	    echo "$line"						\
	    ;;								\
	  esac;								\
	done < doc/quilt.1.in 2>&1 > doc/quilt.1
